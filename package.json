{
  "name": "planning-poker",
  "version": "0.0.0",
  "description": "Planning poker app",
  "private": true,
  "main": "./dist/server.js",
  "scripts": {
    "apollo:generate": "mkdir -p ./src/__generated__ && apollo codegen:generate --target typescript --outputFlat ./src/__generated__/graphql.ts",
    "build-storybook": "build-storybook",
    "build:prod": "NODE_ENV=production npm run build",
    "build:server": "tsc -p ./src/server && npm run copy-schema",
    "build:web": "webpack",
    "build": "npm run build:web && npm run build:server",
    "clean": "rm -rf ./dist",
    "copy-schema": "cp ./src/server/schema.graphql ./dist",
    "heroku-postbuild": "npm run build:prod",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "postinstall": "npm run apollo:generate",
    "start": "heroku local",
    "start:no-ssr": "NO_SSR=true npm start",
    "dev": "npm run build:server && concurrently npm:start:no-ssr \"npm:build:web -- --watch\"",
    "storybook": "start-storybook -p 6006"
  },
  "keywords": [
    "planning",
    "poker"
  ],
  "author": "Xapphire13",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/classnames": "^2.2.9",
    "@types/color": "^3.0.1",
    "@types/hash-sum": "^1.0.0",
    "@types/node-fetch": "^2.5.4",
    "@types/qrcode": "^1.3.4",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-with-styles": "^4.0.2",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "apollo": "^2.22.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "rewiremock": "^3.13.9",
    "simpsons-names": "^2.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.8.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.37",
    "@reach/router": "^1.3.0-beta.0",
    "aphrodite": "^2.4.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-server-express": "^2.14.2",
    "apollo-utilities": "^1.3.3",
    "classnames": "^2.2.6",
    "color": "^3.1.2",
    "delay": "^4.3.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-import": "^1.0.0-beta.2",
    "graphql-tag": "^2.10.1",
    "hash-sum": "^2.0.0",
    "internal-ip": "^5.0.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "pancake-layout": "^0.3.2",
    "prettier": "^1.19.1",
    "qrcode": "^1.4.4",
    "react": "^16.12.0",
    "react-confetti": "^5.0.1",
    "react-dom": "^16.12.0",
    "react-jss": "^10.0.4",
    "react-use": "^13.14.0",
    "react-with-styles": "^4.1.0",
    "react-with-styles-interface-aphrodite": "^6.0.1",
