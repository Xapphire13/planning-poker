{
  "name": "planning-poker",
  "version": "0.0.0",
  "description": "Planning poker app",
  "private": true,
  "main": "./dist/server.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "start": "NODE_ENV=development electron ./dist/server.js",
    "start:debug": "NODE_ENV=development electron --inspect-brk ./dist/server.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pack": "NODE_ENV=production npm run build && electron-builder --dir",
    "dist": "NODE_ENV=production npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src"
  },
  "keywords": [
    "planning",
    "poker"
  ],
  "author": "Xapphire13",
  "license": "ISC",
  "build": {
    "asar": false,
    "appId": "xapphire13.planning-poker",
    "directories": {
      "output": "dist-packages"
    },
    "mac": {
      "category": "public.app-category.business"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/hash-sum": "^1.0.0",
    "@types/node-fetch": "^2.5.4",
    "@types/qrcode": "^1.3.4",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-with-styles": "^4.0.2",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/parser": "^2.14.0",
    "copy-webpack-plugin": "^5.1.1",
    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "simpsons-names": "^2.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.8.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.37",
    "@reach/router": "^1.3.0-beta.0",
    "aphrodite": "^2.4.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-server-express": "^2.9.14",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "hash-sum": "^2.0.0",
    "internal-ip": "^5.0.0",
    "ngrok": "^3.2.7",
    "node-fetch": "^2.6.0",
    "progressbar.js": "^1.1.0",
    "qrcode": "^1.4.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-with-styles": "^4.1.0",
    "react-with-styles-interface-aphrodite": "^6.0.1",
    "uuid": "^3.3.3"
