{
  "name": "planning-poker",
  "version": "0.0.0",
  "description": "Planning poker app",
  "private": true,
  "main": "./dist/server.js",
  "scripts": {
    "apollo:generate": "mkdir -p ./src/__generated__ && apollo codegen:generate --target typescript --outputFlat ./src/__generated__/graphql.ts",
    "build-storybook": "build-storybook",
    "build:prod": "NODE_ENV=production npm run build",
    "build:server": "tsc -p ./src/server && npm run copy-schema",
    "build:web": "webpack",
    "build": "npm run build:web && npm run build:server",
    "clean": "rm -rf ./dist",
    "copy-schema": "cp ./src/server/schema.graphql ./dist",
    "heroku-postbuild": "npm run build:prod",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "postinstall": "npm run apollo:generate",
    "start": "heroku local",
    "start:no-ssr": "NO_SSR=true npm start",
    "dev": "npm run build:server && concurrently npm:start:no-ssr \"npm:build:web -- --watch\"",
    "storybook": "start-storybook -p 6006"
  },
  "keywords": [
    "planning",
    "poker"
  ],
  "author": "Xapphire13",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/classnames": "^2.2.10",
    "@types/color": "^3.0.1",
    "@types/hash-sum": "^1.0.0",
    "@types/node-fetch": "^2.5.7",
    "@types/qrcode": "^1.3.4",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.36",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-with-styles": "^4.0.5",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
